  url('../fonts/bree-bold-webfont.woff2') format('woff2'),
  url('../fonts/bree-bold-webfont.woff') format('woff'),
  url('../fonts/bree-bold-webfont.ttf') format('truetype'),
  url('../fonts/bree-bold-webfont.svg#bree_rgbold') format('svg');
  font-weight: normal;
  font-style: normal;

